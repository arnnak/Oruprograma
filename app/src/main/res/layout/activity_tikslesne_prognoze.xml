<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lenovoz51.oruprograma.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/vietove2"
            android:layout_width="280dp"
            android:layout_height="46dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="40dp"
            android:gravity="center"
            android:text="Vietove" />

        <TextView
            android:id="@+id/aprasymas2"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:text="Aprasymas" />

        <com.android.volley.toolbox.NetworkImageView
            android:id="@+id/ikona2"
            android:layout_width="139dp"
            android:layout_height="113dp"
            android:layout_marginLeft="20dp"
            app:srcCompat="@mipmap/ic_launcher" />

        <TextView
            android:id="@+id/temperatura2"
            android:layout_width="match_parent"
            android:layout_height="33dp"
            android:text="Temperatura" />

        <TextView
            android:id="@+id/vejas2"
            android:layout_width="match_parent"
            android:layout_height="32dp"
            android:text="Vejas" />

        <TextView
            android:id="@+id/debesuotumas2"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text="Debesuotumas" />

        <TextView
            android:id="@+id/slegis2"
            android:layout_width="match_parent"
            android:layout_height="32dp"
            android:text="Slegis" />

        <TextView
            android:id="@+id/dregme2"
            android:layout_width="match_parent"
            android:layout_height="29dp"
            android:text="Dregme" />


        <TextView
            android:id="@+id/atnaujinimas2"
            android:layout_width="match_parent"
            android:layout_height="37dp"
            android:text="Atnaujinimo laikas" />

    </LinearLayout>
</LinearLayout>
